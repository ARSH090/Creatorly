{
    "cleanUrls": true,
    "crons": [
        {
            "path": "/api/workers/process-queue",
            "schedule": "* * * * *"
        },
        {
            "path": "/api/cron/publish",
            "schedule": "*/10 * * * *"
        },
        {
            "path": "/api/cron/refresh-tokens?secret=YOUR_CRON_SECRET_PLACEHOLDER",
            "schedule": "0 3 * * *"
        }
    ]
}